<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pickup_date">

    <ImageView
        android:id="@+id/imageView72"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_group_216" />

    <TextView
        android:id="@+id/textView161"
        style="@style/select_pick"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="255dp"
        android:text="@string/select_pick"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <RadioGroup
        android:layout_width="363dp"
        android:layout_height="183dp"
        android:layout_marginTop="316dp"
        android:layout_marginEnd="16dp"
        android:padding="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <RadioButton
            android:id="@+id/radioButton4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layoutDirection="rtl"
            android:padding="5dp"
            android:text="Book Now"
            android:textAlignment="textStart" />

        <RadioButton
            android:id="@+id/radioButton5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layoutDirection="rtl"
            android:padding="5dp"
            android:text="10:00 - 7:00"
            android:textAlignment="textEnd"
            tools:ignore="RtlCompat" />

        <RadioButton
            android:id="@+id/radioButton6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layoutDirection="rtl"
            android:padding="5dp"
            android:text="1:00 - 10:00"
            android:textAlignment="textEnd"
            tools:ignore="RtlCompat" />

        <RadioButton
            android:id="@+id/radioButton7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layoutDirection="rtl"
            android:padding="5dp"
            android:text="4:00 - 1:00"
            android:textAlignment="textEnd"
            tools:ignore="RtlCompat" />

        <RadioButton
            android:id="@+id/radioButton8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="start"
            android:layoutDirection="rtl"
            android:padding="5dp"
            android:text="7:00 - 4:00"
            android:textAlignment="textEnd"
            tools:ignore="RtlCompat" />
    </RadioGroup>

    <ImageView
        android:id="@+id/imageView73"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="328dp"
        android:background="@color/material_on_background_disabled"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_ellipse_303" />

    <ImageView
        android:id="@+id/imageView75"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="360dp"
        android:background="@color/material_on_background_disabled"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_group_460" />

    <ImageView
        android:id="@+id/imageView76"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="394dp"
        android:background="@color/material_on_background_disabled"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_group_461" />

    <ImageView
        android:id="@+id/imageView77"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="428dp"
        android:background="@color/material_on_background_disabled"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_group_463" />

    <ImageView
        android:id="@+id/imageView78"
        android:layout_width="27dp"
        android:layout_height="27dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="460dp"
        android:background="@color/material_on_background_disabled"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_group_462" />

    <Button
        android:id="@+id/button14"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="#FF2315"
        android:text="@string/save"
        android:textColor="@color/design_default_color_background"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>